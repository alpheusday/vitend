{
    "$schema": "https://typedoc-plugin-markdown.org/schema.json",
    "entryPoints": [
        "./src/index.ts",
        "./src/node.ts",
        "./src/vite.ts"
    ],
    "includeVersion": true,
    "out": "./../apis",
    "cleanOutputDir": true,
    "plugin": [
        "typedoc-plugin-markdown",
        "./../scripts/insert-link.js"
    ],
    "readme": "none",
    "entryFileName": "README",
    "hidePageHeader": true,
    "useCodeBlocks": true,
    "indexFormat": "table"
}
